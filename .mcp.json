{
  "neural-flow": {
    "command": "docker",
    "args": [
      "compose", 
      "exec", 
      "-T",
      "neural-flow", 
      "/app/docker-entrypoint.sh"
    ],
    "env": {
      "PROJECT_NAME": "claude-l9-template",
      "USE_QODO_EMBED": "true",
      "ENABLE_AB_TESTING": "true",
      "ENABLE_PERFORMANCE_MONITORING": "true"
    }
  }
}
