# Project Configuration
PROJECT_NAME=claude-template

# Qdrant Ports (project-specific to avoid conflicts)
# These are calculated based on project path hash
# Default: 6678/6679 for claude-l9-template
QDRANT_REST_PORT=6678
QDRANT_GRPC_PORT=6679

# Performance Settings
OMP_NUM_THREADS=4
MKL_NUM_THREADS=4

# Neural Flow Settings
USE_QODO_EMBED=true
ENABLE_AB_TESTING=true
ENABLE_PERFORMANCE_MONITORING=true

# Note: Enterprise neural v3 uses ports 6333/6334
# Make sure your project ports don't conflict